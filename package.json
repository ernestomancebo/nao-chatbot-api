{
  "name": "nao-chatbot-api",
  "version": "0.0.0",
  "description": "A NAO robot project used as data retriever",
  "main": "dist/index.js",
  "dependencies": {
    "diskdb": "0.1.17",
    "express": "4.17.1",
    "moment": "2.24.0",
    "swagger-ui-express": "4.1.3",
    "tsoa": "2.5.13"
  },
  "devDependencies": {
    "@types/express": "4.17.2",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "@types/swagger-ui-express": "4.1.1",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.2.0",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm test"
    }
  },
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "copy:data": "cp -r -p ./src/data/db/ ./dist/data",
    "build": "tsc && npm run copy:data",
    "prestart": "npm run build",
    "start": "node .",
    "predebug": "npm run build",
    "debug": "node --nolazy --inspect-brk=9229 .",
    "clean:dist": "rm -r ./dist",
    "dev": "nodemon --exec ts-node src/index.ts --watch src",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsoa:routes": "tsoa routes",
    "tsoa:swagger": "tsoa swagger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ernestomancebo/nao-chatbot-api.git"
  },
  "keywords": [
    "nao",
    "nao robot",
    "api"
  ],
  "author": "ernesmancebo@gmail.com",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/ernestomancebo/nao-chatbot-api/issues"
  },
  "homepage": "https://github.com/ernestomancebo/nao-chatbot-api#readme"
}
